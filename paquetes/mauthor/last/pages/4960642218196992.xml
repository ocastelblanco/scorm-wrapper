       } else {\n                    var page = $(".ic_page");\n                    var pageHeight = page.parent().css("height").replace("px", "");\n                    var bodyHeight = $(window).height();\n                    var height = math.min(pageHeight, bodyHeight);\n                    var offset = math.ceil( (height - header - navHeight) / 2) + header;\n                }\n\n                document.documentElement.style.setProperty('--theme-navigation-bar-top', offset + 'px');\n                document.documentElement.style.setProperty('--theme-popup-top', scroll + 'px');\n                document.documentElement.style.setProperty('--theme-popup-center', (math.ceil(height / 2) + scroll) + 'px');\n                document.documentElement.style.setProperty('--theme-popup-bottom', (math.ceil(height) + scroll) + 'px');\n                window['__playerResizeTimmer'] = 0;\n            }, 100);\n        }\n    }\n\n    window.addEventListener('resize', function() {\n        window['__playerResize']();\n    });\n\n    $(".ic_header").parents().on('scroll', function(e) {\n        var scrollTop = $(this).scrollTop();\n        presenter.playerController.getEventBus().sendEvent('CustomScrollEvent', {value: scrollTop});\n    });\n\n}\n\n// you must repeat the data reading when they are not loaded correctly\nif (window.iframeSize.windowInnerHeight === 0) {\n    setTimeout(function (e) {\n        window['__playerResize']();\n    }, 200);\n}\n\nwindow['__playerResize']();\n\n\nvar pageCount = presenter.playerController.getPresentation().getPageCount();\n\nif (pageCount == 1) {\n    var navModule = presenter.playerController.getHeaderModule('__navigation_bar');\n    if(navModule) {\n        $(navModule.getView()).hide();\n    }\n}\n\nSCRIPTEND\nEVENTEND\n\nEVENTSTART\nName:ResizeWindow\nSCRIPTSTART\n\nif (window['__playerResize']) {\n  window['__playerResize']();\n}\n\nSCRIPTEND\nEVENTEND\n\nEVENTSTART\nName:ScrollEvent\nSCRIPTSTART\n\nif (window['__playerResize']) {\n  presenter.playerController.getEventBus().sendEvent('CustomScrollEvent', {value: event.value});\n}\n\nSCRIPTEND\nEVENTEND\n\nEVENTSTART\nName:CustomScrollEvent\nSCRIPTSTART\n\nif (window['__playerResize']) {\n  window['__playerResize']();\n}\n\nSCRIPTEND\nEVENTEND</property><property name="Is disabled" displayName="Is Disabled" type="boolean" value=""/></properties></addonModule></modules><groups></groups><editorRulers><vertical>0</vertical></editorRulers><page-weight value='1' mode='defaultWeight'></page-weight></page>